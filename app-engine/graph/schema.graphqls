scalar Time
scalar Decimal

interface Node {
  id: ID!
}

type Query {
  items: [Item!]!
  shoppingItems(after: Time!, before: Time!, item: ID!): [ShoppingItem!]!
  purchases(before: Time!, after: Time!): [Shopping!]!
  vendors: [Vendor!]!
  node(id: Int!): Node!
}

type Mutation {
  createItem(input: ItemInput!): Item
  createPurchase(input: ShoppingInput!): Shopping
}
